{
  "ci": {
    "collect": {
      "url": "https://example.com",
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "resource-summary:script:size": ["error", {"maxNumericValue": 2000000}],
        "resource-summary:stylesheet:size": ["error", {"maxNumericValue": 150000}]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
